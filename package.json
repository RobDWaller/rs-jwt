{
  "name": "rs-jwt",
  "version": "0.1.0-alpha.2",
  "description": "A really simple JSON Web Token reader.",
  "main": "dist/rs-jwt.js",
  "types": "dist/rs-jwt.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "mocha -r ts-node/register ./tests/**/test.**.ts",
    "build": "tsc",
    "analyse": "eslint ./src/** ./tests/**",
    "coverage": "nyc --lines 100 mocha -r ts-node/register ./tests/**/test.**.ts",
    "report": "nyc --reporter=html --reporter=clover mocha -r ts-node/register ./tests/**/test.**.ts",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobDWaller/rs-jwt.git"
  },
  "keywords": [
    "jwt"
  ],
  "author": "Rob Waller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobDWaller/rs-jwt/issues"
  },
  "homepage": "https://github.com/RobDWaller/rs-jwt#readme",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "chai": "^4.2.0",
    "codecov": "^3.6.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  }
}
